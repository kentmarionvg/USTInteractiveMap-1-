<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.ustsitemap.ustinteractivemap.MapScreen"
    android:background="@drawable/titlescreenbg3">


    <ImageButton
        android:layout_width="33dp"
        android:layout_height="36dp"
        android:id="@+id/ibArch"
        android:longClickable="false"
        android:background="@drawable/archicon"
        android:layout_column="11"
        android:layout_marginBottom="101dp"
        android:layout_marginLeft="4dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/ibMain"
        android:layout_alignStart="@+id/ibMain"
        android:onClick="buttonToArch"/>

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:id="@+id/ibMain"
        android:cropToPadding="false"
        android:clickable="false"
        android:baselineAlignBottom="false"
        android:adjustViewBounds="false"
        android:layout_column="10"
        android:layout_gravity="right"
        android:layout_marginBottom="127dp"
        android:layout_above="@+id/ibArch"
        android:layout_centerHorizontal="true"
        android:alpha="0"
        android:onClick="buttonToMain"/>

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/button_custom"
        android:onClick="mapToList" />

    <ImageButton
        android:layout_width="33dp"
        android:layout_height="36dp"
        android:id="@+id/ibEngg"
        android:background="@drawable/enggicon"
        android:layout_alignLeft="@+id/imageButton"
        android:layout_alignStart="@+id/imageButton"
        android:layout_marginTop="335dp"
        android:onClick="buttonToEngg"/>

    <ImageButton
        android:layout_width="33dp"
        android:layout_height="36dp"
        android:id="@+id/ibArki"
        android:layout_alignBottom="@+id/ibArch"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/arkicon"
        android:onClick="buttonToArki"/>

    <ImageButton
        android:layout_width="33dp"
        android:layout_height="36dp"
        android:id="@+id/ibAcc"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/ibEngg"
        android:layout_marginRight="20dp"
        android:background="@drawable/accicon"
        android:onClick="buttonToAcc"/>

    <ImageButton
        android:layout_width="33dp"
        android:layout_height="36dp"
        android:id="@+id/ibCom"
        android:layout_marginBottom="33dp"
        android:layout_above="@+id/ibMain"
        android:layout_toLeftOf="@+id/ibAcc"
        android:layout_marginRight="8dp"
        android:background="@drawable/comicon"
        android:onClick="buttonToCom"/>

    <ImageButton
        android:layout_width="33dp"
        android:layout_height="36dp"
        android:id="@+id/ibEduc"
        android:layout_below="@+id/ibAcc"
        android:layout_alignRight="@+id/ibEngg"
        android:layout_alignEnd="@+id/ibEngg"
        android:background="@drawable/educicon"
        android:onClick="buttonToEduc"/>


</RelativeLayout>
